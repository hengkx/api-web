{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react boilerplate",
  "scripts": {
    "start": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.prod.js --progress --profile --colors"
  },
  "keywords": [
    "react",
    "redux",
    "babel",
    "webpack"
  ],
  "author": {
    "name": "Tom Xu",
    "email": "ycxzhkx@gmail.com",
    "url": "https://github.com/hengkx"
  },
  "license": "MIT",
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 3 versions",
          "> 1%"
        ]
      }
    }
  },
  "dependencies": {
    "antd": "^2.11.0",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "jsencrypt": "^2.3.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "nprogress": "^0.2.0",
    "object.omit": "^2.0.1",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-nprogress": "^0.1.3",
    "redux-reqs": "^0.1.0",
    "redux-saga": "^0.15.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-recharts": "^1.1.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-compat": "^1.0.3",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "postcss-loader": "^2.0.5",
    "postcss-pxtorem": "^4.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.18.2",
    "svg-sprite-loader": "^3.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}
