{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 7"],
        "node": "current"
      },
      "useBuiltIns": true
    }],
    "stage-0",
    "react"
    ],
    "plugins": [
        "add-module-exports",
        "transform-decorators-legacy",
        ["import",{ "libraryName": "antd", "style": true}]
    ],
    "env": {
        "production": {
        },
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        }
    }
}
